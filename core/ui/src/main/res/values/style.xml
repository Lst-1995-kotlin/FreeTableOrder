<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tonalButton" parent="Widget.Material3.Button.TonalButton">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="font">@font/nanumsquareneo_bold</item>
    </style>
    <style name="outlinedButton" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="font">@font/nanumsquareneo_bold</item>
    </style>
    <style name="floatingButton" parent="Widget.Material3.FloatingActionButton.Primary">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="font">@font/nanumsquareneo_bold</item>
    </style>
    <style name="defaultButton" parent="Widget.Material3.Button">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="font">@font/nanumsquareneo_bold</item>
    </style>
    <style name="layout">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
    </style>

    <style name="toolBar" parent="Widget.Material3.Toolbar">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="navigationIcon">?android:attr/actionModeCloseDrawable</item>
        <item name="titleMargin">0dp</item>
        <item name="titleTextAppearance">@style/ToolbarTitleTextAppearance</item>
    </style>

    <style name="ToolbarTitleTextAppearance" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/text_large</item>
        <item name="fontFamily">@font/nanumsquareneo_bold</item>
    </style>

    <style name="cardView" parent="Widget.Material3.CardView.Elevated">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="elevation">@dimen/elevation</item>
        <item name="cardCornerRadius">@dimen/cardCornerRadius</item>
    </style>

    <style name="textView" parent="Widget.MaterialComponents.TextView">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="font">@font/nanumsquareneo_regular</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_large</item>
    </style>
    <style name="textInputEditText" parent="Widget.Material3.TextInputEditText.FilledBox">
        <item name="android:theme">@style/Theme.FreeTableOrder</item>
        <item name="font">@font/nanumsquareneo_regular</item>
        <item name="android:textSize">@dimen/text_large</item>
    </style>
</resources>